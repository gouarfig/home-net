{
	"name": "home-nest",
	"version": "0.0.1",
	"description": "Home-Nest project",
	"repository": {
		"type": "git",
		"url": "https://github.com/gouarfig/homenest.git"
	},
	"scripts": {
		"start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
		"lite": "lite-server",
		"postinstall": "typings install",
		"tsc": "tsc",
		"tsc:w": "tsc -w",
		"typings": "typings",
		"lite-server-test": "lite-server --config=liteserver-test-config.json",
		"test": "tsc && concurrently \"npm run tsc:w\" \"npm run lite-server-test\" ",
		"install:phpunit": "cd ./php/tests && ./install_phpunit.sh",
		"phpunit": "cd ./php/tests && ./run_tests.sh",
		"install:composer": "cd ./php/tests && ./install_composer.sh",
		"clean:js": "rimraf app/*.js & rimraf app/*.js.map",
		"setowner": "chown -R www-data.www-data ../homenest",
		"clean": "rimraf dist",
		"build": "npm run clean && tsc",
		"browserify": "browserify -s main app/main.js > dist/bundle.js",
		"minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
		"build_prod": "npm run build && npm run browserify && npm run minify",
		"webpack": "webpack-dev-server --inline --progress --port 8080",
		"build_webpack": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail"
	},
	"license": "ISC",
	"dependencies": {
		"@angular/common": "2.0.0-rc.5",
		"@angular/compiler": "2.0.0-rc.5",
		"@angular/core": "2.0.0-rc.5",
		"@angular/forms": "0.3.0",
		"@angular/http": "2.0.0-rc.5",
		"@angular/platform-browser": "2.0.0-rc.5",
		"@angular/platform-browser-dynamic": "2.0.0-rc.5",
		"@angular/router": "3.0.0-rc.1",
		"@angular/router-deprecated": "2.0.0-rc.2",
		"@angular/upgrade": "2.0.0-rc.5",
		"systemjs": "~0.19.27",
		"core-js": "^2.4.0",
		"reflect-metadata": "^0.1.3",
		"rxjs": "~5.0.0-beta.6",
		"zone.js": "^0.6.13",
		"angular2-in-memory-web-api": "0.0.15",
		"jquery": "~1.12.1",
		"bootstrap": "^3.3.6",
		"highcharts": "~4.2.6",
		"angular2-highcharts": "~0.1.1",
		"font-awesome": "*",
		"weathericons": "*"
	},
	"devDependencies": {
		"rimraf": "^2.5.4",
		"concurrently": "^2.0.0",
		"lite-server": "^2.2.0",
		"typescript": "^1.8.10",
		"typings":"^1.0.4",
		"jasmine-core": "~2.4.1",
		
		"angular2-template-loader": "^0.4.0",
		"css-loader": "^0.23.1",
		"extract-text-webpack-plugin": "^1.0.1",
		"file-loader": "^0.8.5",
		"html-loader": "^0.4.3",
		"html-webpack-plugin": "^2.15.0",
		"null-loader": "^0.1.1",
		"raw-loader": "^0.5.1",
		"style-loader": "^0.13.1",
		"ts-loader": "^0.8.1",
		"webpack": "^1.13.0",
		"webpack-dev-server": "^1.14.0",
		"webpack-merge": "^0.14.0"
	}
}